{
    "name": "august6th/amzn-sp-api",
    "description": "Amazon Selling Partner API SDK",
    "keywords": [
        "swagger",
        "php",
        "sdk",
        "api",
        "amazon",
        "sp-api",
        "selling partner api"
    ],
    "license": "proprietary",
    "require": {
        "php": ">=5.5",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "guzzlehttp/guzzle": "^6.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.8",
        "squizlabs/php_codesniffer": "~2.6",
        "friendsofphp/php-cs-fixer": "~2.12"
    },
    "autoload": {
        "psr-4": { "SellingPartnerApi\\" : "lib/" }
    },
    "autoload-dev": {
        "psr-4": { "SellingPartnerApi\\Tests\\" : "test/" }
    }
}
